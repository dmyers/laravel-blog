{
    "name": "mmanos/laravel-blog",
    "description": "A simple blogging system for Laravel 4.",
    "keywords": ["laravel", "blog", "blog post", "blog posts", "blogging"],
    "authors": [
        {
            "name": "Mark Manos",
            "email": "mark@airpac.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.x",
        "michelf/php-markdown": "1.4.*@dev",
        "mmanos/laravel-casset": "1.4.*",
        "dmyers/laravel-storage": "1.0.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/controllers"
        ],
        "psr-0": {
            "Mmanos\\Blog": "src/"
        }
    },
    "minimum-stability": "dev",
    "license": "MIT"
}
